[Encryption]
keep_pagination: True   ; resume encryption from last execution (modifies offset)
blocksize: 47           ; generates a 140 characters string for twitter
offset: 0               ; start enconding from blocksize*offset char
interval= 60            ; in seconds. Twitter allows 2400 twetts per day

[Twitter]
